<kmodule xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://jboss.org/kie/6.0.0/kmodule">

    <kbase name="kBasic" packages="basic">
        <ksession name="kSession"/>
    </kbase>
    
    <kbase name="cepConfigKbase" eventProcessingMode="stream" packages="cep">
        <ksession name="cepConfigKsessionPseudoClock" clockType="pseudo"/>
        <ksession name="cepConfigKsessionRealtimeClock" clockType="realtime"/>
    </kbase>

    <kbase name="backwardKBase" eventProcessingMode="stream" packages="backward">
        <ksession name="backwardSession"/>
    </kbase>

    <kbase name="template-simple-KBase" packages="template">
        <ksession name="templateSimpleKsession"/>
    </kbase>

</kmodule>